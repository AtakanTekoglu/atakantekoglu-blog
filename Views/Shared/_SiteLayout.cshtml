<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Atakan Tekoğlu</title>

    <!-- SEO -->
    <meta name="keywords" content="@ViewBag.ContentSEO.Keywords"
    <meta name="description" content="@Html.Raw(ViewBag.ContentSEO.Description)">
    <meta name="author" content="uipasta">
    <meta name="url" content="http://atakantekoglu.com">
    <meta name="copyright" content="company name">
    <meta name="robots" content="index,follow">

    <!-- Favicon -->
    <link rel="shortcut icon" href="@ViewBag.ContentSEO.LogoURL">
    @*<link rel="apple-touch-icon" sizes="144x144" type="image/x-icon" href="~/Content/SiteLayout/DevBlog/images/favicon/apple-touch-icon.png">*@

    <!-- All CSS Plugins -->
    <link rel="stylesheet" type="text/css" href="~/Content/SiteLayout/DevBlog/css/plugin.css">

    <!-- Main CSS Stylesheet -->
    <link rel="stylesheet" type="text/css" href="~/Content/SiteLayout/DevBlog/css/style.css">

    <!-- Google Web Fonts  -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:400,300,500,600,700">


    <!-- HTML5 shiv and Respond.js support IE8 or Older for HTML5 elements and media queries -->
    <!--[if lt IE 9]>
